{
  "kind": "collectionType",
  "collectionName": "projekts",
  "info": {
    "singularName": "projekt",
    "pluralName": "projekts",
    "displayName": "Projekt",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Titel": {
      "type": "string",
      "required": true
    },
    "Beschreibung": {
      "type": "text",
      "required": true
    },
    "Bilder": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "positionOnLandingPage": {
      "type": "integer",
      "min": 0,
      "unique": true,
      "max": 18
    },
    "landingPageImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "portrait",
        "corporate",
        "interior",
        "jewellery"
      ],
      "required": true
    },
    "positionOnCategoryPage": {
      "type": "integer",
      "min": 0,
      "required": false,
      "default": 0,
      "max": 36
    },
    "fullscreen": {
      "type": "boolean",
      "default": false
    },
    "involved": {
      "displayName": "Beteiligte",
      "type": "component",
      "repeatable": true,
      "component": "beteiligte.beteiligte"
    },
    "allPageImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    }
  }
}
